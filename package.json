{
  "name": "prague-shenanigans",
  "version": "0.0.1",
  "description": "Prague Shenanigans Application",
  "productName": "Prague Shenanigans",
  "author": "Marian Adamus <adamus.marian@email.cz>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue src",
    "type-check": "vue-tsc --project tsconfig.vue-tsc.json --noEmit",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:and": "quasar dev -m capacitor -T android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "build": "quasar build",
    "build:and": "quasar build -m capacitor -T android",
    "build:ios": "quasar build -m capacitor -T ios",
    "gt": "npx swagger-typescript-api -p http://localhost:5015/swagger/v1/swagger.json -o ./src/types/generated -n generated.types.ts --extract-request-body --no-client --extract-request-params --clean-output --extract-enums --silent"
  },
  "dependencies": {
    "@capacitor/android": "^7.1.0",
    "@capacitor/geolocation": "^7.1.1",
    "@intlify/vue-i18n-bridge": "^1.1.0",
