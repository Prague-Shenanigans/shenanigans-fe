    "@vueuse/core": "^13.0.0",
    "axios": "^1.2.1",
    "dompurify": "^3.2.4",
    "luxon": "^3.4.4",
    "mapbox-gl": "^3.11.1",
    "marked": "^15.0.7",
    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "swagger-typescript-api": "^13.0.5",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.18",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.1.0",
